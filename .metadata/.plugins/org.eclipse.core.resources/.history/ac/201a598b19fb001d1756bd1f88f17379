package leekangjo;

import nl.captcha.text.producer.TextProducer;

public class setTextProducer implements TextProducer {
	 private final String srcStr;
	 public setTextProducer(String answer) {   
	 srcStr = answer;
	 }
	 
	//@Override
	   public String getText() {        
	        return srcStr;
	   }
}
