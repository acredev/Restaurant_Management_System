<body>
	<input type="text" placeholder=" *아이디" id="testid" maxlength="10">
					<script type="text/javascript">
							function idChk()
							{
								var idchk = document.getElementById('testid');
								
								if (idchk.value == """)
									{
										alert(idchk.value + "아이디를 입력해주세요.");
										
									}
							}
						</script>
					<input type="button" onclick="idChk()" value="중복확인">
</body>
